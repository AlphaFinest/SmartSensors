@model IEnumerable<SmartSensors.Areas.Admin.Models.RegisterSensorsViewModel>
@{
    ViewBag.Title = "RegisterSensors";
}

<h2>Table of all sensors</h2>
@section JavaScript
    {
    <script type="text/javascript">
        var clients = [];
    @foreach (var mos in Model)
    {
            
                @:clients.push({"Username": "@mos.Owner.UserName", "SensorName":"@mos.Name" , "Value":"@mos.Value"})
            
    }

        $("#jsGrid").jsGrid({
            width: "100%",
            height: "400px",

            inserting: false,
            editing: false,
            sorting: true,
            paging: true,

            data: clients,

            fields: [
                { name: "Username", type: "text", width: 150, validate: "required" },
                { name: "SensorName", type: "text", width: 50 },
                { name: "Value", type: "text", width: 200 },
                { name: "Status",type:}
            ]
        });

    </script>
}

<div id="jsGrid"></div>


